{
  "AllowedHosts": "*",
  "General": {
    "MinAgeInMonth": 3,
    "MobileNumberLength": 10
  },
  "Serilog": {
    "Using": [],
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning",
        "System": "Warning"
      },
      "Enrich": [
        "FromLogContext",
        "WithMachineName",
        "WithProcessId",
        "WithThread"
      ],
      "WriteTo": [
        {
          "Name": "Console"
        },
        {
          "Name": "File",
          "Args": {
            "path": "C:\\Africa\\LogFile\\Munharaunda\\Log.txt",
            "outputTemplate": "{Timestamp:G} {message}{NewLine:1}{Exception:1}"
          }
        }
      ]
    }
  }
}